- event: command_palette_opened
  schema:
    palette_id: id
    trigger_method: keyboard | button | menu = keyboard
    opened_at: date = now()

- event: command_palette_closed
  schema:
    palette_id: id
    close_reason: selection | escape | blur = escape
    time_open_ms: int? #min(0)
    closed_at: date = now()

- event: command_selected
  schema:
    palette_id: id
    command_id: id
    command_name: string?
    command_category: string?
    selection_index: int? #min(0)
    selected_at: date = now()

- event: command_searched
  schema:
    palette_id: id
    search_query: string
    results_count: int? #min(0)
    searched_at: date = now()
