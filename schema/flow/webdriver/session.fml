- action: new_session
  inputs:
    browser: chrome | firefox | edge | safari | opera = chrome
    capabilities: web_driver_capabilities
    headless: boolean = false
    server_url: string
  outputs:
    browser: string
    session_id: string

- action: quit
  inputs:
    session_id: string
- action: status
  inputs:
    session_id: string
  outputs:
    message: string
    ready: boolean
