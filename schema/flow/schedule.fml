- action: create_schedule
  inputs:
    cron_expression: string
    description: string
    enabled: boolean = true
    metadata: schedule_metadata
    name: string
    timezone: string = utc
  outputs:
    name: string
    next_run: int
    schedule_id: string

- action: update_schedule
  inputs:
    cron_expression: string
    description: string
    enabled: boolean
    metadata: schedule_metadata
    name: string
    schedule_id: string
    timezone: string
  outputs:
    next_run: int
    schedule_id: string

- action: delete_schedule
  inputs:
    schedule_id: string
  outputs:
    schedule_id: string

- action: enable_schedule
  inputs:
    schedule_id: string
  outputs:
    enabled: boolean
    next_run: int
    schedule_id: string

- action: disable_schedule
  inputs:
    schedule_id: string
  outputs:
    enabled: boolean
    schedule_id: string

- action: get_schedule
  inputs:
    schedule_id: string
  outputs:
    created_at: int
    cron_expression: string
    enabled: boolean
    last_run: int
    metadata: schedule_metadata
    name: string
    next_run: int
    schedule_id: string
    timezone: string

- action: list_schedules
  inputs:
    enabled: boolean
    limit: int = 100
    offset: int = 0
    search: string
  outputs:
    schedules: list[schedule_info]
    total: int

- action: trigger_schedule
  inputs:
    schedule_id: string
  outputs:
    execution_id: string
    schedule_id: string
    triggered_at: int

- action: get_next_run
  inputs:
    count: int = 1
    schedule_id: string
  outputs:
    next_runs: list[integer]
    schedule_id: string

- action: validate_cron
  inputs:
    cron_expression: string
    timezone: string = utc
  outputs:
    error: string
    next_run: int
    valid: boolean

- action: create_one_time
  inputs:
    description: string
    metadata: schedule_metadata
    name: string
    run_at: int
    timezone: string = utc
  outputs:
    run_at: int
    schedule_id: string

- action: pause_schedule
  inputs:
    resume_at: int
    schedule_id: string
  outputs:
    paused: boolean
    resume_at: int
    schedule_id: string

- action: resume_schedule
  inputs:
    schedule_id: string
  outputs:
    next_run: int
    paused: boolean
    schedule_id: string

- action: get_schedule_history
  inputs:
    end_date: int
    limit: int = 100
    schedule_id: string
    start_date: int
  outputs:
    executions: list[schedule_execution]
    schedule_id: string
    total: int

- action: get_schedule_stats
  inputs:
    end_date: int
    schedule_id: string
    start_date: int
  outputs:
    average_duration: float
    failed_executions: int
    last_execution: schedule_execution
    schedule_id: string
    successful_executions: int
    total_executions: int

- action: bulk_enable
  inputs:
    schedule_ids: list[string]
  outputs:
    failed: int
    results: list[schedule_bulk_result]
    successful: int

- action: bulk_disable
  inputs:
    schedule_ids: list[string]
  outputs:
    failed: int
    results: list[schedule_bulk_result]
    successful: int

- action: create_interval
  inputs:
    description: string
    enabled: boolean = true
    end_time: int
    interval_seconds: int
    name: string
    start_time: int
  outputs:
    next_run: int
    schedule_id: string

- action: update_action
  inputs:
    action_config: schedule_action_config
    action_type: webhook | function | job
    schedule_id: string
  outputs:
    schedule_id: string

- object: schedule
  schema:
    schedule_id: string
    name: string
    cron_expression: string
    timezone: string
    enabled: boolean
    next_run: date
    last_run: date
    metadata: schedule_metadata

- object: schedule_metadata
  schema:
    values: map[string, any]

- object: schedule_info
  schema:
    schedule_id: string
    name: string
    cron_expression: string
    enabled: boolean
    next_run: int
    last_run: int

- object: schedule_execution
  schema:
    execution_id: string
    schedule_id: string
    started_at: int
    completed_at: int
    status: string
    duration_ms: int
    error: string

- object: schedule_bulk_result
  schema:
    schedule_id: string
    error: string

- object: schedule_action_config
  schema:
    url: string
    method: string
    headers: map[string, string]
    body: any
