- action: create_meeting
  inputs:
    agenda: string
    duration: int = 60
    password: string
    recurrence: zoom_recurrence
    settings: zoom_meeting_settings
    start_time: date
    timezone: string
    topic: string
    type: instant | scheduled | recurring_fixed | recurring_unfixed = scheduled
  outputs:
    join_url: string
    meeting_id: string
    password: string
    start_url: string

- action: get_meeting
  inputs:
    meeting_id: string
  outputs:
    agenda: string
    duration: int
    host_id: string
    join_url: string
    meeting_id: string
    password: string
    start_time: date
    status: string
    timezone: string
    topic: string
    type: int

- action: update_meeting
  inputs:
    agenda: string
    duration: int
    meeting_id: string
    password: string
    settings: zoom_meeting_settings
    start_time: date
    timezone: string
    topic: string
    type: instant | scheduled | recurring_fixed | recurring_unfixed

- action: delete_meeting
  inputs:
    meeting_id: string
    occurrence_id: string
    schedule_for_reminder: boolean = false

- action: list_meetings
  inputs:
    next_page_token: string
    page_size: int = 30
    type: scheduled | live | upcoming | upcoming_meetings | previous_meetings = scheduled
    user_id: string = "me"
  outputs:
    meetings: list[zoom_meeting]
    next_page_token: string
    total_records: int

- action: add_registrant
  inputs:
    custom_questions: list[zoom_custom_question]
    email: string
    first_name: string
    last_name: string
    meeting_id: string
    phone: string
  outputs:
    join_url: string
    registrant_id: string

- action: list_registrants
  inputs:
    meeting_id: string
    next_page_token: string
    page_size: int = 30
    status: pending | approved | denied = approved
  outputs:
    next_page_token: string
    registrants: list[zoom_registrant]
    total_records: int

- action: update_registrant_status
  inputs:
    method: approve | deny | cancel
    meeting_id: string
    registrants: list[zoom_registrant_id]

- action: get_participants
  inputs:
    meeting_id: string
    next_page_token: string
    page_size: int = 30
    type: past | past_one | live = live
  outputs:
    next_page_token: string
    participants: list[zoom_participant]
    total_records: int

- action: create_webinar
  inputs:
    agenda: string
    duration: int = 60
    password: string
    settings: zoom_webinar_settings
    start_time: date
    timezone: string
    topic: string
    type: webinar | recurring_webinar_fixed | recurring_webinar_unfixed = webinar
  outputs:
    join_url: string
    password: string
    start_url: string
    webinar_id: string

- action: get_webinar
  inputs:
    webinar_id: string
  outputs:
    agenda: string
    duration: int
    host_id: string
    join_url: string
    start_time: date
    timezone: string
    topic: string
    type: int
    webinar_id: string

- action: delete_webinar
  inputs:
    occurrence_id: string
    webinar_id: string

- action: get_user
  inputs:
    user_id: string = "me"
  outputs:
    created_at: date
    email: string
    first_name: string
    last_name: string
    status: string
    timezone: string
    type: int
    user_id: string

- action: list_users
  inputs:
    next_page_token: string
    page_size: int = 30
    role_id: string
    status: active | inactive | pending = active
  outputs:
    next_page_token: string
    total_records: int
    users: list[zoom_user]

- action: get_recording
  inputs:
    meeting_id: string
  outputs:
    download_url: string
    duration: int
    recording_files: list[zoom_recording_file]
    share_url: string

- action: delete_recording
  inputs:
    method: trash | delete = trash
    meeting_id: string

- action: list_recordings
  inputs:
    from_date: date
    next_page_token: string
    page_size: int = 30
    to_date: date
    user_id: string = "me"
  outputs:
    next_page_token: string
    recordings: list[zoom_recording]
    total_records: int

- action: end_meeting
  inputs:
    meeting_id: string

- action: send_invitation
  inputs:
    emails: list[string]
    meeting_id: string

- object: zoom_meeting
  schema:
    meeting_id: string
    topic: string
    type: int
    start_time: date
    duration: int
    timezone: string
    password: string
    join_url: string
    start_url: string
    host_id: string
    status: string

- object: zoom_recurrence
  schema:
    type: daily | weekly | monthly
    repeat_interval: int
    weekly_days: string
    monthly_day: int
    monthly_week: int
    monthly_week_day: int
    end_times: int
    end_date_time: date

- object: zoom_meeting_settings
  schema:
    host_video: boolean
    participant_video: boolean
    join_before_host: boolean
    mute_upon_entry: boolean
    watermark: boolean
    use_pmi: boolean
    approval_type: int
    registration_type: int
    audio: string
    auto_recording: string
    waiting_room: boolean

- object: zoom_webinar_settings
  schema:
    host_video: boolean
    panelists_video: boolean
    practice_session: boolean
    hd_video: boolean
    approval_type: int
    registration_type: int
    audio: string
    auto_recording: string
    close_registration: boolean
    show_share_button: boolean
    allow_multiple_devices: boolean

- object: zoom_custom_question
  schema:
    title: string
    value: string

- object: zoom_registrant
  schema:
    id: string
    email: string
    first_name: string
    last_name: string
    status: string
    create_time: date
    join_url: string

- object: zoom_registrant_id
  schema:
    id: string
    email: string

- object: zoom_participant
  schema:
    id: string
    user_id: string
    name: string
    email: string
    join_time: date
    leave_time: date
    duration: int
    attentiveness_score: string
    status: string

- object: zoom_user
  schema:
    id: string
    email: string
    first_name: string
    last_name: string
    type: int
    status: string
    created_at: date
    timezone: string

- object: zoom_recording_file
  schema:
    id: string
    meeting_id: string
    recording_start: date
    recording_end: date
    file_type: string
    file_size: int
    download_url: string
    play_url: string
    status: string

- object: zoom_recording
  schema:
    id: string
    meeting_id: string
    topic: string
    type: int
    start_time: date
    duration: int
    total_size: int
    recording_count: int
    recording_files: list[zoom_recording_file]

- object: zoom_meeting_registrant
  schema:
    email: string
    first_name: string
    join_url: string
    last_name: string
    meeting_id: string
    phone: string
    registered_at: date = now()
    registrant_id: string
    status: pending | approved | denied | cancelled = approved

- object: zoom_meeting_participant
  schema:
    duration_seconds: int
    email: string
    join_time: date = now()
    leave_time: date
    meeting_id: string
    name: string
    user_id: string

- object: zoom_webinar
  schema:
    agenda: string
    created_at: date = now()
    duration: int = 60
    host_id: string
    join_url: string
    start_time: date
    timezone: string
    topic: string
    type: webinar | recurring_webinar_fixed | recurring_webinar_unfixed = webinar
    updated_at: date = now()
    webinar_id: string
