- action: uuid
  inputs:
    version: v4 | v7 = v4
  outputs:
    uuid: string

- action: bytes
  inputs:
    length: int
  outputs:
    bytes: bytes

- action: string
  inputs:
    charset: alphanumeric | alpha | numeric | hex = alphanumeric
    length: int
  outputs:
    string: string

- action: number
  inputs:
    integer: boolean = true
    max: float
    min: float
  outputs:
    number: float

- action: choice
  inputs:
    items: list[any]
  outputs:
    item: any

- action: shuffle
  inputs:
    items: list[any]
  outputs:
    items: list[any]

- object: random_value
  schema:
    seed: int
    value: any
    type: string

- object: random_seed
  schema:
    algorithm: string = default
    created_at: date = now()
    seed_value: int
