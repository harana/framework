- webobject: feed_item_tag
  id: [feed_item_id, name]
  attributes:
    - color: red | orange | yellow | green | blue | indigo | purple | pink | gray = gray
    - feed_item_id: id -> ui_feed_item.id
    - href: string? #max(500)
    - label: string #max(100)
    - order: int = 0 #min(0)
  events:
    - click
