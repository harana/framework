- webobject: dropdown_item
  id: [dropdown_id, name]
  attributes:
    - description: string? #max(500)
    - dropdown_id: id -> ui_dropdown.id
    - href: string? #max(500)
    - icon: string? #max(100)
    - is_disabled: boolean = false
    - label: string #max(200)
    - order: int = 0 #min(0)
    - section_id: id? -> ui_dropdown_section.id
    - shortcut: string? #max(20)
  events:
    - click